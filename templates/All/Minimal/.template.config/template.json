{
    "$schema": "http://json.schemastore.org/template",
    "author": "RussBaz",
    "classifications": [
        "Web",
        "WebFrame",
        "Minimal"
    ],
    "identity": "WebFrameTemplate.MinimalProject",
    "name": "WebFrame Templates: Minimal Project",
    "shortName": "webframe",
    "tags": {
        "language": "F#",
        "type": "project"
    },
    "sourceName": "Minimal",
    "preferNameDirectory": true,
    "symbols": {
        "Framework": {
            "type": "parameter",
            "description": "The target framework for the project.",
            "datatype": "choice",
            "choices": [
                {
                    "choice": "net5.0",
                    "description": "Target .NET 5"
                },
                {
                    "choice": "net6.0",
                    "description": "Target .NET 6"
                }
            ],
            "defaultValue": "net5.0",
            "replaces": "net5.0"
        }
    }
}